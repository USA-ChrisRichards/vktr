  <%= form_for @user do |f| %>
  <p>
  <%= f.label :username %><br>
  <%= f.text_field :username %>
  </p>
  <p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
  </p>
  <p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label :location %><br>
  <%= f.text_field :location %>
  </p> 
  <p>
  <%= f.label :image_link %><br>
  <%= f.text_field :img_url %>
  </p>
  <p>
  <%= f.submit %>
  </p>
  <% end %>